{
    "name": "validus/cache",
    "description": "Provides PSR-6 and PSR-16 cache implementation facotirs for your Expressive project.",
    "type": "library",
    "homepage": "https://github.com/ValidusPHP/cache",
    "license": "MIT",
    "keywords": [
        "zend-expressive",
        "symfony-cache",
        "container-interop-cache",
        "psr-11",
        "psr-16",
        "psr-6",
        "factories",
        "cache"
    ],
    "require": {
        "php": "^7.1",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^4.1"
    },
    "require-dev": {
        "doctrine/dbal": "^2.8",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.10.3",
        "phpstan/phpstan-strict-rules": "^0.10.1",
        "phpunit/phpunit": "^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Validus\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Validus\\Tests\\Cache\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "phpstan analyze -c ./phpstan.neon",
        "fix": "php-cs-fixer fix -vvv",
        "test": "phpunit --colors=always --coverage-clover ./tests/logs/clover.xml",
        "coveralls": "php-coveralls -v",
        "check": [
            "@fix",
            "@analyze",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "config-provider": "Validus\\Cache\\ConfigProvider"
        }
    }
}
